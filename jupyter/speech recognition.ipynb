    "import speech_recognition as sr\n",
    "r = sr.Recognizer()\n",
    "mic = sr.Microphone()\n",
    "with mic as source:\n",
    "    print(\"starting recording\")\n",
    "    r.adjust_for_ambient_noise(source)\n",
    "    audio = r.listen(source)\n",
    "    print(audio)    \n",
    "    s = r.recognize_google(audio, language=\"fr-FR\")\n",
    "    print(s)\n"
   ]
  }
