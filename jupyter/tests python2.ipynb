    "from websocket import create_connection\n",
    "\n",
    "ws = create_connection(\"ws://localhost:8000/ws\")\n",
    "print \"Sending 'Hello, World'...\"\n",
    "ws.send(\"Hello, World\")\n",
    "print \"Sent\"\n",
    "print \"Receiving...\"\n",
    "result = ws.recv()\n",
    "print \"Received '%s'\" % result\n",
    "ws.close()"
   ]
